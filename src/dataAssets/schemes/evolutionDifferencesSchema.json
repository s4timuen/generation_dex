{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "generations": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "generation": {
                        "type": "string"
                    },
                    "trigger": {
                        "type": "string"
                    },
                    "method": {
                        "type": "string"
                    },
                    "requirement": {
                        "type": "string"
                    }
                },
                "minProperties": 1,
                "maxProperties": 8,
                "additionalProperties": false
            }
        }
    },
    "minProperties": 2,
    "maxProperties": 2,
    "required": [
        "name",
        "generations"
    ],
    "additionalProperties": false
}
